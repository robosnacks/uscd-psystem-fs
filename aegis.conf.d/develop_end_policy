develop_end_policy_command =
    "aede-policy --project=$project --change=$change";

project_specific =
[
    {
        name = "aede-policy";
        value =
            "authors "
            "copyright "
            "comments "
            "crlf "
            "description "
            "escape-hyphen "
            "fsf-address "
            "gpl-version "
            "line-length "
            "man-pages "
            "merge-fhist "
            "merge-rcs "
            "no-manifest "
            "no-tabs "
            "printable "
            "text "
            "vim-mode "
            "white-space"
            ;
    },
];

unchanged_file_develop_end_policy = error;
unchanged_file_integrate_pass_policy = warning;


